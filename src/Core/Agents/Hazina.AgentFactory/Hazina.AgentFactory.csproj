<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <LanguageVersion>latest</LanguageVersion>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <RootNamespace>Hazina.AgentFactory</RootNamespace>
    <Version>2.0.0</Version>
    <PackageVersion>2.0.0</PackageVersion>
    <Authors>Prospergenics</Authors>
    <Description>High-level agent factory for building autonomous AI agents in Hazina. Provides configuration parsing, built-in tool collections (file ops, git, dotnet, npm, BigQuery, email, WordPress), and multi-agent flow orchestration. Simplifies creating production-ready AI agents.</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/prospergenics/devgpt</PackageProjectUrl>
    <RepositoryUrl>https://github.com/prospergenics/devgpt</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>ai-agents;agent-factory;orchestration;tools;bigquery;email;wordpress;git;automation</PackageTags>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Google.Cloud.BigQuery.V2" Version="3.11.0" />
    <PackageReference Include="MailKit" Version="4.13.0" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\..\LLMs\Hazina.LLMs.Classes\Hazina.LLMs.Classes.csproj" />
    <ProjectReference Include="..\..\Storage\Hazina.Store.DocumentStore\Hazina.Store.DocumentStore.csproj" />
    <ProjectReference Include="..\..\Storage\Hazina.Store.EmbeddingStore\Hazina.Store.EmbeddingStore.csproj" />
    <ProjectReference Include="..\Hazina.Generator\Hazina.Generator.csproj" />
    <ProjectReference Include="..\..\LLMs\Hazina.LLMs.Helpers\Hazina.LLMs.Helpers.csproj" />
    <ProjectReference Include="..\..\LLMs\Hazina.LLMs.Client\Hazina.LLMs.Client.csproj" />
    <ProjectReference Include="..\..\LLMs.Providers\Hazina.LLMs.OpenAI\Hazina.LLMs.OpenAI.csproj" />
    <ProjectReference Include="..\..\LLMs.Providers\Hazina.LLMs.SemanticKernel\Hazina.LLMs.SemanticKernel.csproj" />
  </ItemGroup>
</Project>